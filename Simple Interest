import java.util.Scanner;
public class Simple
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in);
        int p= sc.nextInt();
        int t= sc.nextInt();
        float r= sc.nextFloat();
        float a=p*r*t/100;
        System.out.printf("%.2f",a);
    }
}
