import java.util.Scanner;
public class Series6
{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int a= sc.nextInt();
        int c,d;
        for(int i=1;i<=a;i++) {
            if(i%2==0){
                c=(i*i)-2;
                System.out.printf("%d ",c);
            }
            else if(i%2==1){
                d=(i*i)-1;
                System.out.printf("%d ",d);
            }
        }
    }
}
