import java.util.Scanner;
public class Lcm {
  public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int n1 = a, n2 = b , lcm;

    lcm = (n1 > n2) ? n1 : n2 ;

    while(true) {
      if( lcm % n1 == 0 && lcm % n2 == 0 ) {
        System.out.printf("LCM of %d and %d is %d.", n1, n2,lcm);
        break;
      }
      ++lcm;
    }
  }
}
