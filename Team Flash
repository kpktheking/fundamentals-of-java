import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();  
        int y1=sc.nextInt(); 
        int x2=sc.nextInt(); 
        int y2=sc.nextInt(); 
        float a=x1+x2;
        float b=y1+y2;
        float c=a/2;
        float d=b/2;
        System.out.println(c);
        System.out.println(d);
        
    }
}
