import java.util.Scanner;
public class Flash
{
    public static void main(String args[])
    {
       Scanner sc=new Scanner(System.in);
       int x1=sc.nextInt();
       int y1=sc.nextInt();
       int x2=sc.nextInt();
       int y2=sc.nextInt();
       int x3=sc.nextInt();
       int y3=sc.nextInt();
       float a=x1+x2+x3; 
       float b=y1+y2+y3;
       float c=a/3;
       float d=b/3;
       System.out.printf("%.1f\n",c);
       System.out.printf("%.1f\n",d);
        
    }
}
