import java.util.Scanner;
public class Palindrome
{
    public static void main(String[] args)
    {
     Scanner sc=new Scanner(System.in);
  int r,sum=0,temp;    
  int a=sc.nextInt();
  int n=a;
  
  temp=n;    
  while(n>0){    
   r=n%10;  
   sum=(sum*10)+r;    
   n=n/10;    
  }    
  if(temp==sum)    
   System.out.println("palindrome");    
  else    
   System.out.println("Not a palindrome");    
}  
}
