import java.util.Scanner;
public class Area
{
    public static void main(String args[])
    {
        Scanner sc= new Scanner(System.in);
        int s= sc.nextInt();
        int lr= sc.nextInt();
        int br= sc.nextInt();
        float r= sc.nextFloat();
        int a=s*s;
        int b=lr*br;
        double c=3.14*r*r;
        System.out.println(a);
        System.out.println(b);
        System.out.printf("%.2f",c);
    }
}
